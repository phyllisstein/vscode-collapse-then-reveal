{
	"name": "collapse-then-reveal",
	"displayName": "Collapse Then Reveal",
	"description": "Automatically collapse inactive Explorer paths.",
	"version": "0.0.1",
	"license": "GPL-3.0-or-later",
	"engines": {
		"vscode": "^1.33.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
